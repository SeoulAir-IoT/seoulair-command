{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MqttOptions": {
    "NotificationsConnection": {
      "BrokerAddress": "eclipse-mosquitto",
      "BrokerPort": 1883,
      "Topic": "seoulair/notifications",
      "SenderClientId": "seoul-air-command-notification-sender",
      "MaxNumOfAttemptsToSendMessage": 3
    }
  },
  "AllowedHosts": "*"
}
